-- Main combined loader

-- Callback for after the loading screen finishes
getgenv().AfterLoading = function()
    -- Load your main game script
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Seuj09/X-scripts/refs/heads/main/Script.lua", true))()
end

-- Run the loading screen
loadstring(game:HttpGet("https://raw.githubusercontent.com/Seuj09/Deltablockgui/refs/heads/main/Gui.lua", true))()
